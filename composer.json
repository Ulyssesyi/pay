{
    "name": "yijin/pay",
    "description": "A project for diff channel to pay in china",
    "type": "library",
    "version": "1.0.3",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^7.3",
        "nesbot/carbon": "^2.16",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Yijin\\Pay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yijin",
            "email": "yijin@91laiqian.com"
        }
    ],
    "minimum-stability":"dev",
    "license":"Apache-2.0",
    "scripts": {
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src ./examples"
    }
}
